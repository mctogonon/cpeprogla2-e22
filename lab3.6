#include <cstring>
#include<iostream>

using namespace std;


char stri(char t[100]){
	int k,txt;
	for(k=0; t[k]!='\0'; k++);
	for(k--; k>=0;k--){
		if(t[k-1]==' ' || k==0){
			for(txt=k; t[txt]!='\0' && t[txt]!=' '; txt++)
      			cout<<t[txt];
      			cout<<endl;
		}
 	}		
 	cout << endl;

}


main(){
	char str1[30];
	cout << endl;
	cout << "Enter a string: ";
	cin.getline(str1, 30);
	stri(str1);
}

